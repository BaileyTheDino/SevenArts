root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = tab
trim_trailing_whitespace = true
max_line_length = off
ij_any_align_multiline_parameters = false
ij_any_align_multiline_parameters_in_calls = false

[*.php]
indent_size = 4
indent_style = space

[COMMIT_EDITMSG]
max_line_length = 0
